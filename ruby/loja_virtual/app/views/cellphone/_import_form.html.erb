<div class="container">

<%= form_tag({action: :import}, multipart: true) do %>
	<div class="form-group">
		<label for="file">CSV File</label>
  		<%= file_field_tag 'file', class: 'form-control-file' %>
	</div>

	<div class="form-group">
 		<%= submit_tag "Submit", class: 'btn btn-primary' %>
 	</div>	
<% end %>

</div>
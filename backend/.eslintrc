{
	"env": {
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "script"
	},
	"rules": {
		"object-shorthand": [
			"warn"
		],
		"quotes": [
			"warn",
			"single"
		],
		"no-console": [
			"error"
		],
		"linebreak-style": [
			0
		],
		"max-len": [
			0,
			120,
			4
		]
	}
}
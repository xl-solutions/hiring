{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"prettier",
		"eslint:recommended",
		"plugin:react/recommended"
	
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"prettier"
	],
	"rules": {
		"prettier/prettier": [
			"error",
			{
			  "endOfLine": "auto"
			}
		  ],
		"react/jsx-filename-extension": [
		  "warn", 
		  {"extensions": [".js", ".jsx"]}
		]

	  }
}